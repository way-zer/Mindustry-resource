{
  "name": "mindustry-resource",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "renderer/server.ts",
  "scripts": {
    "dev": "vite",
    "check": "vue-tsc --noEmit",
    "serve": "vite preview",
    "run:prod": "vite-node src/util/viteSSR/prodServer.ts",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/util/viteSSR/prodServer.ts"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.30.0",
    "@monaco-editor/loader": "^1.3.2",
    "@vueuse/head": "^0.9.8",
    "axios": "^1.1.3",
    "clipboard": "^2.0.11",
    "connect": "^3.7.0",
    "core-js": "^3.26.0",
    "element-plus": "^2.2.19",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.23",
    "pinia-class-store": "^1.1.1",
    "select": "^1.1.2",
    "ses": "^0.15.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/fa": "^1.1.2",
    "@types/connect": "^3.4.35",
    "@types/node": "^17.0.45",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "monaco-editor": "^0.33.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylus": "^0.59.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.1.8",
    "vite-node": "^0.24.3",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-pwa": "^0.13.1",
    "vue-tsc": "^1.0.9",
    "workbox-window": "^6.5.4"
  },
  "packageManager": "pnpm@7.5.0+",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  }
}
