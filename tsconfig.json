{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "astro/client",
      "vite-plugin-pwa/client"
    ],
    "jsx": "preserve",
    "baseUrl": ".",
    "outDir": "dist/",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@services/*": [
        "src/services/*"
      ]
    }
  }
}